{
  "name": "junior-portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "debug": "^4.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "jquery": "^3.3.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-particles-js": "^2.4.2",
    "react-router": "^4.3.1",
    "react-scripts": "2.1.1",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint app.js"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "line-break-style": "none",
      "no-unnamed-func": 0,
      "arrow-body-style": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/no-unescaped-entities": 0,
      "no-unused-expressions": 0,
      "prefer-const": 0,
      "react/destructuring-assignment": 1
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
